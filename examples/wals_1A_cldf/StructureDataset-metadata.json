{
    "@context": "http://www.w3.org/ns/csvw", 
    "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#StructureDataset", 
    "dc:source": "sources.bib", 
    "dialect": {
        "commentPrefix": null
    }, 
    "tables": [
        {
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#ValueTable", 
            "tableSchema": {
                "columns": [
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://purl.org/dc/terms/identifier", 
                        "required": true, 
                        "name": "ID"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://linguistics-ontology.org/gold/2010/Language", 
                        "required": true, 
                        "name": "Language_ID"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://www.w3.org/2004/02/skos/core#Concept", 
                        "required": true, 
                        "name": "Parameter_ID", 
                        "titles": "Feature_ID"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://purl.org/linguistics/gold/dataStructureRelation", 
                        "required": true, 
                        "name": "Value"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://purl.org/dc/terms/description", 
                        "name": "Comment"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://purl.org/dc/terms/source", 
                        "separator": ";", 
                        "name": "Source"
                    }
                ], 
                "foreignKeys": [
                    {
                        "columnReference": [
                            "Language_ID"
                        ], 
                        "reference": {
                            "resource": "languages.csv", 
                            "columnReference": [
                                "ID"
                            ]
                        }
                    }, 
                    {
                        "columnReference": [
                            "Parameter_ID"
                        ], 
                        "reference": {
                            "resource": "parameters.csv", 
                            "columnReference": [
                                "ID"
                            ]
                        }
                    }
                ], 
                "primaryKey": [
                    "ID"
                ]
            }, 
            "url": "values.csv"
        }, 
        {
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#LanguageTable", 
            "tableSchema": {
                "columns": [
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://purl.org/dc/terms/identifier", 
                        "name": "ID"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Name"
                    }, 
                    {
                        "datatype": {
                            "base": "decimal", 
                            "minimum": -90, 
                            "maximum": 90
                        }, 
                        "propertyUrl": "http://www.w3.org/2003/01/geo/wgs84_pos#lat", 
                        "name": "Latitude"
                    }, 
                    {
                        "datatype": {
                            "base": "decimal", 
                            "minimum": -180, 
                            "maximum": 180
                        }, 
                        "propertyUrl": "http://www.w3.org/2003/01/geo/wgs84_pos#long", 
                        "name": "Longitude"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#glottocode", 
                        "name": "glottocode"
                    }, 
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#iso639P3code", 
                        "name": "iso639P3code"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Genus"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Family"
                    }
                ], 
                "primaryKey": [
                    "ID"
                ]
            }, 
            "url": "languages.csv"
        }, 
        {
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#ParameterTable", 
            "tableSchema": {
                "columns": [
                    {
                        "datatype": "string", 
                        "propertyUrl": "http://purl.org/dc/terms/identifier", 
                        "name": "ID"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Name"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Authors"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Url"
                    }, 
                    {
                        "datatype": "string", 
                        "name": "Area"
                    }
                ], 
                "primaryKey": [
                    "ID"
                ]
            }, 
            "url": "parameters.csv"
        }
    ]
}