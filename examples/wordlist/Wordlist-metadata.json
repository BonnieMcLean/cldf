{
    "@context": [
        "http://www.w3.org/ns/csvw",
        {
            "@language": "en"
        }
    ],
    "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#Wordlist",
    "tables": [
        {
            "url": "forms.csv",
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#FormTable",
            "tableSchema": {
                "columns": [
                    {
                        "name": "ID",
                        "datatype": "integer",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#id"
                    },
                    {
                        "name": "Language_ID",
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#languageReference"
                    },
                    {
                        "name": "Language",
                        "datatype": "string",
                        "propertyUrl": "http://purl.org/linguistics/gold/Language"
                    },
                    {
                        "name": "Concept",
                        "datatype": "string",
                        "propertyUrl": "http://purl.org/linguistics/gold/LexicalizedConcept"
                    },
                    {
                        "name": "Parameter_ID",
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#parameterReference",
                        "valueUrl": "http://concepticon.clld.org/parameters/{Parameter_ID}"
                    },
                    {
                        "name": "Value",
                        "datatype": "string"
                    },
                    {
                        "name": "Form",
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#form"
                    },
                    {
                        "name": "Segments",
                        "datatype": "string",
                        "separator": " + ",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#segments"
                    },
                    {
                        "name": "Prosodic_Structure",
                        "datatype": "string",
                        "separator": " ",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#prosodicStructure"
                    },
                    {
                        "name": "Source",
                        "datatype": "string",
                        "separator": ",",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#source",
                        "valueUrl": "http://evobib.lingpy.org?key={Source}"
                    }
                ]
            }
        },
        {
            "url": "cognates.csv",
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#CognateTable",
            "tableSchema": {
                "columns": [
                    {
                        "name": "Form_ID",
                        "datatype": "integer",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#formReference"
                    },
                    {
                        "name": "Form",
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#form"
                    },
                    {
                        "name": "Cognateset_ID",
                        "datatype": "string",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#cognatesetReference"
                    },
                    {
                        "name": "Alignment",
                        "datatype": "string",
                        "separator": " ",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#alignment"
                    },
                    {
                        "name": "Doubt",
                        "datatype": "string"
                    },
                    {
                        "name": "Cognate_detection_method",
                        "datatype": "string"
                    },
                    {
                        "name": "Cognate_source",
                        "separator": ",",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#source",
                        "datatype": "string"
                    },
                    {
                        "name": "Alignment_method",
                        "datatype": "string"
                    },
                    {
                        "name": "Alignment_source",
                        "datatype": "string"
                    },
                    {
                        "name": "Segment_slice",
                        "datatype": {"base": "string", "format": "\\d(:\\d(:\\d)?)?"},
                        "separator": " ",
                        "propertyUrl": "http://cldf.clld.org/v1.0/terms.rdf#segmentSlice"
                    }
                ],
                "foreignKeys": [
                    {
                        "columnReference": "Form_ID",
                        "reference": {
                            "resource": "forms.csv",
                            "columnReference": "ID"
                        }
                    }
                ]
            }
        }
    ]
} 
