{
	"url": "examples.csv",
	"dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#ExampleTable",
	"dc:format": "http://linguistics-ontology.org/gold/InterlinearGlossedText",
	"tableSchema": {
		"columns": [
			{
				"name": "ID",
				"propertyUrl": "http://purl.org/dc/terms/identifier",
				"datatype": "string"
			},
			{
				"name": "Language_ID",
				"propertyUrl": "http://purl.org/linguistics/gold/Language",
				"required": true,
				"datatype": "string"
			},
			{
				"name": "Primary",
				"propertyUrl": "http://purl.org/linguistics/gold/hasSourceLine",
				"datatype": "string"
			},
			{
				"name": "Analyzed",
				"datatype": "string",
				"propertyUrl": "http://purl.org/linguistics/gold/GrammarUnit",
				"separator": "\t"
			},
			{
				"name": "Gloss",
				"propertyUrl": "http://purl.org/linguistics/gold/hasGlosses",
				"datatype": "string",
				"separator": "\t"
			},
			{
				"name": "Translation",
				"propertyUrl": "http://purl.org/linguistics/gold/hasTranslationLine",
				"datatype": "string"
			},
			{
				"name": "Translation_language_ID",
				"propertyUrl": "http://purl.org/linguistics/gold/Language",
				"datatype": "string"
			}
		],
		"primaryKey": "ID"
	}
}
